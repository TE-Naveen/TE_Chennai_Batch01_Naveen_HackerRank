import java.util.*;
interface AdvancedArithmetic{
  int divisor_sum(int n);
}
class MyCalculator implements AdvancedArithmetic{
  public int divisor_sum(int n){
        int num=0;
        for(int i=1;i<=n/2;i++){
            if(n%i==0){
            num=i+num;
            }
        }
        return num+n;
    }
}
